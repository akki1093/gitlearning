<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Mobile Menu"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <columns
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns"
                margin="{Boolean}true">
                <items jcr:primaryType="nt:unstructured">
                    <column
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/container">
                        <items jcr:primaryType="nt:unstructured">
                            <mobile-menu-list
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                composite="{Boolean}true"
                                fieldLabel="Mobile Menu List">
                                <field
                                    jcr:primaryType="nt:unstructured"
                                    sling:resourceType="granite/ui/components/coral/foundation/container"
                                    name="./mobileMenuList">
                                    <items jcr:primaryType="nt:unstructured">
                                        <column
                                            jcr:primaryType="nt:unstructured"
                                            sling:resourceType="granite/ui/components/coral/foundation/container">
                                            <items jcr:primaryType="nt:unstructured">
                                                <title
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                    fieldDescription="Can not be empty"
                                                    fieldLabel="Title"
                                                    name="./title"/>
                                                <link
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                    fieldDescription="Links the title. Path to a content page, external URL or page anchor."
                                                    fieldLabel="URL"
                                                    name="./link"
                                                    rootPath="/content/boighor"/>
                                                <isParent
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                    fieldDescription="Can not be empty"
                                                    name="./isParent"
                                                    text="Does it contains child list?"
                                                    value="yes"/>
                                                <inner-list
                                                    jcr:primaryType="nt:unstructured"
                                                    sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                    composite="{Boolean}true"
                                                    fieldLabel="Inner List">
                                                    <field
                                                        jcr:primaryType="nt:unstructured"
                                                        sling:resourceType="granite/ui/components/coral/foundation/container"
                                                        name="./mobileMenuInnerList">
                                                        <items jcr:primaryType="nt:unstructured">
                                                            <column
                                                                jcr:primaryType="nt:unstructured"
                                                                sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                <items jcr:primaryType="nt:unstructured">
                                                                    <title
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                        fieldDescription="Can not be empty"
                                                                        fieldLabel="Title"
                                                                        name="./title"/>
                                                                    <link
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                        fieldDescription="Links the title. Path to a content page, external URL or page anchor."
                                                                        fieldLabel="URL"
                                                                        name="./link"
                                                                        rootPath="/content/boighor"/>
                                                                    <isParent
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/checkbox"
                                                                        fieldDescription="Can not be empty"
                                                                        name="./isParent"
                                                                        text="Does it contains child list?"
                                                                        value="yes"/>
                                                                    <sub-inner-list
                                                                        jcr:primaryType="nt:unstructured"
                                                                        sling:resourceType="granite/ui/components/coral/foundation/form/multifield"
                                                                        composite="{Boolean}true"
                                                                        fieldLabel="Sub Inner List">
                                                                        <field
                                                                            jcr:primaryType="nt:unstructured"
                                                                            sling:resourceType="granite/ui/components/coral/foundation/container"
                                                                            name="./mobileMenuSubInnerList">
                                                                            <items jcr:primaryType="nt:unstructured">
                                                                                <column
                                                                                    jcr:primaryType="nt:unstructured"
                                                                                    sling:resourceType="granite/ui/components/coral/foundation/container">
                                                                                    <items jcr:primaryType="nt:unstructured">
                                                                                        <title
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                                                                                            fieldDescription="Can not be empty"
                                                                                            fieldLabel="Title"
                                                                                            name="./title"/>
                                                                                        <link
                                                                                            jcr:primaryType="nt:unstructured"
                                                                                            sling:resourceType="cq/gui/components/coral/common/form/pagefield"
                                                                                            fieldDescription="Links the title. Path to a content page, external URL or page anchor."
                                                                                            fieldLabel="URL"
                                                                                            name="./link"
                                                                                            rootPath="/content/boighor"/>
                                                                                    </items>
                                                                                </column>
                                                                            </items>
                                                                        </field>
                                                                    </sub-inner-list>
                                                                </items>
                                                            </column>
                                                        </items>
                                                    </field>
                                                </inner-list>
                                            </items>
                                        </column>
                                    </items>
                                </field>
                            </mobile-menu-list>
                        </items>
                    </column>
                </items>
            </columns>
        </items>
    </content>
</jcr:root>
